{
  "compilerOptions": {
    "outDir": "./dist/",
    "baseUrl": ".",
    "paths": {
      "@common/*": ["./src/common/*"],
      "@bkd/*": ["./src/bkd/*"],
      "@image-to-asm/*": ["./src/image-to-asm/*"],
      "@wav-converter/*": ["./src/wav-converter/*"]
    },
    "noImplicitAny": false,
    "sourceMap": true,
    "lib": [
      "es2015",
      "dom"
    ],
    "module": "es6",
    "esModuleInterop": true,
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "downlevelIteration": true,
    "moduleResolution": "node"
  }
}